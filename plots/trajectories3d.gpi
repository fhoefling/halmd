#
# Trajectories for Molecular Dynamics Simulation of a Lennard-Jones fluid
#

set terminal png size 960,720
set output '@CMAKE_CURRENT_BINARY_DIR@/trajectories3d.png'

unset key
unset title
set xlabel 'x'
set ylabel 'y'
set zlabel 'z'

splot '@CMAKE_CURRENT_BINARY_DIR@/data/trajectories3d.dat' using 1:2:3 with points pt 7
