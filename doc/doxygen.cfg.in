DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = @PROJECT_NAME@
PROJECT_NUMBER         = @PROGRAM_VERSION@
PROJECT_BRIEF          = @PROGRAM_DESC@
PROJECT_LOGO           = @CMAKE_CURRENT_SOURCE_DIR@/_static/halmd_logo_small.png
GENERATE_LATEX         = NO
GENERATE_MAN           = NO
GENERATE_RTF           = NO
CASE_SENSE_NAMES       = NO
QUIET                  = YES
JAVADOC_AUTOBRIEF      = YES
GENERATE_HTML          = YES
HTML_OUTPUT            = @CMAKE_CURRENT_BINARY_DIR@/html/reference
GENERATE_XML           = NO
XML_PROGRAMLISTING     = NO
INPUT                  = @CMAKE_SOURCE_DIR@/halmd @CMAKE_SOURCE_DIR@/include
STRIP_FROM_PATH        = @CMAKE_SOURCE_DIR@
RECURSIVE              = YES
FULL_PATH_NAMES        = YES
INPUT_ENCODING         = UTF-8
FILE_PATTERNS          = *.cpp *.hpp *.cu *.cuh
EXCLUDE_PATTERNS       = */deprecated/* */test/*
HIDE_UNDOC_MEMBERS     = YES
HIDE_UNDOC_CLASSES     = YES
SORT_MEMBER_DOCS       = YES
SORT_BRIEF_DOCS        = YES
SORT_GROUP_NAMES       = YES
SORT_BY_SCOPE_NAME     = YES
SORT_MEMBERS_CTORS_1ST = YES
VERBATIM_HEADERS       = NO
