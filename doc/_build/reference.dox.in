DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = @PROJECT_NAME@
PROJECT_NUMBER         = @PROGRAM_VERSION@
GENERATE_LATEX         = NO
GENERATE_MAN           = NO
GENERATE_RTF           = NO
CASE_SENSE_NAMES       = NO
QUIET                  = YES
JAVADOC_AUTOBRIEF      = YES
GENERATE_HTML          = NO
GENERATE_XML           = YES
XML_PROGRAMLISTING     = NO
INPUT                  = @CMAKE_SOURCE_DIR@/src/halmd
STRIP_FROM_PATH        = @CMAKE_SOURCE_DIR@/src
RECURSIVE              = YES
FULL_PATH_NAMES        = YES
INPUT_ENCODING         = UTF-8
FILE_PATTERNS          = *.cpp *.hpp *.cu *.cuh
EXCLUDE_PATTERNS       = */deprecated/* */test/*
EXTRACT_ALL            = YES
HIDE_UNDOC_MEMBERS     = NO
HIDE_UNDOC_CLASSES     = NO
